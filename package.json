{
    "name": "GetterAndSetterGenerator",
    "displayName": "Getter and Setter Generator",
    "description": "This extension generate get and set methods from the class variable declarations.",
    "version": "1.0.4",
    "publisher": "yunfei",
    "repository": 
    {
        "type": "git",
        "url": "https://github.com/STRUDYGRASS/getter-setter-generator"    
    },
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.generateGetterAndSetters"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.generateGetterAndSetters",
            "title": "Generate get and set methods"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}